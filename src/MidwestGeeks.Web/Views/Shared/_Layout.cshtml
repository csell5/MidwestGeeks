<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/date.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/2011.3.1129/kendo.all.min.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/Content/MarketPlace.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/2011.3.1129/kendo.common.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/2011.3.1129/kendo.default.min.css")" rel="stylesheet" />
</head>

<body>
<!-- wrap starts here -->
<div id="wrap">

    <!--header -->
	<div id="header">			
		<div id="header-links">
            <p>
                @Html.ActionLink("Home", "Index", "Home") |  
                @Html.ActionLink("My Events", "Index", "Events") | 
                @Html.ActionLink("Map", "Index", "Map") |  
                @Html.ActionLink("Groups", "Index", "Groups") | 
                @Html.ActionLink("About", "About", "Home")
            </p>
		</div>		
	<!--header ends-->				
	</div>
    <div id="siteHeader">Midwest Geeks</div>
	@RenderSection("UpcomingEvents", false)

	<!-- content-wrap starts -->
	<div id="content-wrap" class="two-col"  >	
		<div id="sidebar">
				
			<h1>Sidebar Menu</h1>
			<ul class="sidemenu">				
				<li><a href="http://www.anglebracket.net">angleBracket</a></li>
				<li><a href="http://www.lcnug.org">LCNUG</a></li>
				<li><a href="http://www.chicagoalt.net">Chicago Alt.Net</a></li>
			</ul>	
				
			<h1>Links</h1>
			<ul class="sidemenu">
				<li>Get Some Links</li>
			</ul>
			
			<h1>Sponsors</h1>
			<ul class="sidemenu">
                <li>Get some sponsors</li>
			</ul>
			
			<h1>Wise Words</h1>
			<p>&quot;I put some unit tests in your unit tests so you can unit test your unit tests while unit testing.&quot;</p>
			<p class="align-right">- Xunit</p>
					
		<!-- sidebar ends -->		
		</div>
		
		<div id="main">
            @RenderBody()
		</div>
		
	<!-- content-wrap ends-->	
	</div>
		
	<!-- footer starts -->			
	<div id="footer-wrap"><div id="footer">				
			
            <p>
			&copy; 2010 Your Company

